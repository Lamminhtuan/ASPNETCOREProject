            <div class="row">
                <div class="col-8 col-sm-6">
                    <!-- InstanceBeginEditable name="author avt" --><img src="/css_news4/636925656081349181_hinh-anh-dai-dien (1).jpg" class="rounded-circle" align="left">
                    <!-- InstanceEndEditable -->

